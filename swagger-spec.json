{
  "components": {
    "schemas": {
      "CreatePostDto": {
        "properties": {
          "content": { "type": "string" },
          "title": { "description": "The title of the post.", "type": "string" }
        },
        "required": ["title", "content"],
        "type": "object"
      },
      "ListPostDto": {
        "properties": {
          "limit": { "default": 20, "example": 20, "type": "number" },
          "offset": { "default": 0, "example": 0, "type": "number" },
          "order": {
            "default": "desc",
            "enum": ["asc", "desc"],
            "example": "asc",
            "type": "string"
          },
          "orderBy": {
            "default": "createdAt",
            "enum": ["createdAt", "updatedAt"],
            "example": "createdAt",
            "type": "string"
          }
        },
        "type": "object"
      },
      "ListPostResponse": {
        "properties": {
          "pagination": { "$ref": "#/components/schemas/PaginationDto" },
          "posts": {
            "items": { "$ref": "#/components/schemas/PostResponse" },
            "type": "array"
          },
          "totalCount": { "type": "number" }
        },
        "required": ["posts", "totalCount", "pagination"],
        "type": "object"
      },
      "PaginationDto": {
        "properties": {
          "page": { "type": "number" },
          "perPage": { "type": "number" }
        },
        "required": ["page", "perPage"],
        "type": "object"
      },
      "PostResponse": {
        "properties": {
          "authorId": { "type": "string" },
          "content": { "type": "string" },
          "createdAt": { "format": "date-time", "type": "string" },
          "id": { "type": "string" },
          "removedAt": { "type": "object" },
          "title": { "type": "string" },
          "updatedAt": { "format": "date-time", "type": "string" }
        },
        "required": [
          "id",
          "authorId",
          "title",
          "content",
          "createdAt",
          "updatedAt"
        ],
        "type": "object"
      },
      "RemovePostsDto": { "properties": {}, "type": "object" },
      "UpdateMeDto": {
        "properties": { "name": { "type": "string" } },
        "required": ["name"],
        "type": "object"
      },
      "UpdatePostDto": {
        "properties": {
          "content": { "type": "object" },
          "title": { "type": "object" }
        },
        "type": "object"
      }
    }
  },
  "info": {
    "contact": {},
    "description": "user and post api description",
    "title": "nestjs template",
    "version": "1.0"
  },
  "openapi": "3.0.0",
  "paths": {
    "/posts": {
      "delete": {
        "operationId": "PostController_removePosts",
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/RemovePostsDto" }
            }
          },
          "required": true
        },
        "responses": { "200": { "description": "" } },
        "tags": ["posts"]
      },
      "get": {
        "operationId": "PostController_listPost",
        "parameters": [
          {
            "example": 20,
            "in": "query",
            "name": "limit",
            "required": false,
            "schema": { "default": 20, "type": "number" }
          },
          {
            "example": 0,
            "in": "query",
            "name": "offset",
            "required": false,
            "schema": { "default": 0, "type": "number" }
          },
          {
            "example": "asc",
            "in": "query",
            "name": "order",
            "required": false,
            "schema": {
              "default": "desc",
              "enum": ["asc", "desc"],
              "type": "string"
            }
          },
          {
            "example": "createdAt",
            "in": "query",
            "name": "orderBy",
            "required": false,
            "schema": {
              "default": "createdAt",
              "enum": ["createdAt", "updatedAt"],
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/ListPostDto" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ListPostResponse" }
              }
            },
            "description": "List of posts"
          }
        },
        "tags": ["posts"]
      },
      "post": {
        "operationId": "PostController_createPost",
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/CreatePostDto" }
            }
          },
          "required": true
        },
        "responses": { "200": { "description": "" } },
        "tags": ["posts"]
      }
    },
    "/posts/{id}": {
      "get": {
        "operationId": "PostController_getPost",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": { "type": "string" }
          }
        ],
        "responses": { "200": { "description": "" } },
        "tags": ["posts"]
      },
      "put": {
        "operationId": "PostController_putPost",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": { "type": "string" }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/UpdatePostDto" }
            }
          },
          "required": true
        },
        "responses": { "200": { "description": "" } },
        "tags": ["posts"]
      }
    },
    "/users/me": {
      "get": {
        "operationId": "UserController_getMe",
        "parameters": [],
        "responses": { "200": { "description": "" } },
        "tags": ["users"]
      },
      "patch": {
        "operationId": "UserController_updateMe",
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/UpdateMeDto" }
            }
          },
          "required": true
        },
        "responses": { "200": { "description": "" } },
        "tags": ["users"]
      }
    }
  },
  "servers": [],
  "tags": []
}
