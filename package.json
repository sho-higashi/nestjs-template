{
  "dependencies": {
    "@nestjs/common": "10.3.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.0",
    "@nestjs/platform-express": "10.3.0",
    "@nestjs/swagger": "7.1.17",
    "@nestjs/terminus": "10.2.0",
    "@prisma/client": "5.7.1",
    "express": "4.18.2",
    "nestjs-cls": "3.6.0",
    "reflect-metadata": "0.1.14",
    "rxjs": "7.8.1",
    "ts-pattern": "5.0.6",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "10.2.1",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.3.0",
    "@redocly/cli": "1.6.0",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.101",
    "@swc/jest": "0.2.29",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.6",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "chokidar-cli": "3.0.0",
    "concurrently": "8.2.2",
    "cspell": "8.2.4",
    "dependency-cruiser": "16.0.0",
    "dotenv": "16.3.1",
    "dotenv-cli": "7.3.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-prettier": "5.1.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "3.1.0",
    "git-cz": "4.9.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.2.0",
    "npm-package-json-lint": "7.1.0",
    "npm-package-json-lint-config-default": "6.0.0",
    "prettier": "3.1.1",
    "prettier-plugin-sort-json": "3.1.0",
    "prisma": "5.7.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3",
    "zx": "7.2.3"
  },
  "engines": {
    "node": "20.x",
    "pnpm": "8.x"
  },
  "name": "nestjs-template",
  "packageManager": "pnpm@8.14.0",
  "prisma": {
    "seed": "ts-node --swc prisma/seed.ts"
  },
  "private": true,
  "scripts": {
    "build": "nest build -b swc",
    "clean": "rm -rf dist",
    "commit": "git-cz",
    "commit:all": "git add . && pnpm commit",
    "dev": "concurrently -n app,doc \"pnpm dev:app\" \"pnpm dev:doc\" \"pnpm prisma studio --browser=none\"",
    "dev:app": "nest start -b swc --watch",
    "dev:doc": "chokidar ./src/openapi-spec.json -c \"pnpm format:doc\"",
    "format": "concurrently \"npm:format:*\"",
    "format:code": "prettier --write .*/*.{js,json} *.{js,ts,json,yml} ./**/*.{js,ts,json,yml} --plugin=prettier-plugin-sort-json",
    "format:db": "prisma format",
    "format:doc": "prettier --write ./src/openapi-spec.json --plugin=prettier-plugin-sort-json",
    "gen:db": "prisma generate",
    "lint": "concurrently \"npm:lint:*\"",
    "lint:cspell": "cspell -c cspell.json \"**\" \".*\" \".*/**\" --no-progress",
    "lint:db": "zx ./scripts/lint-db.mjs",
    "lint:dep": "depcruise --config .dependency-cruiser.js src",
    "lint:doc": "redocly lint --config redocly.yaml src/openapi-spec.json",
    "lint:eslint": "eslint \"{prisma,src,test}/**/*.ts\" \"./**/*.{js,json}\"",
    "lint:package": "npmPkgJsonLint --configFile ./npmpackagejsonlint.config.js .",
    "lint:prettier": "prettier --check .*/*.{js,json} *.{js,ts,json,yml} ./**/*.{js,ts,json,yml} --plugin=prettier-plugin-sort-json",
    "lint:type": "tsc --noEmit",
    "prepare": "test -d node_modules/husky && husky install || echo \"husky is not installed\"",
    "prepare:test:e2e": "dotenv -e .env.test -- prisma migrate reset --force",
    "reset:db": "prisma migrate reset --force",
    "start": "nest start",
    "test": "jest -c ./jest.config.ts . --runInBand",
    "test:e2e": "jest -c ./jest.config.ts test --runInBand",
    "test:ut": "jest -c ./jest.config.ts src"
  },
  "version": "0.0.1"
}
