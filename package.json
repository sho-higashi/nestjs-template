{
  "dependencies": {
    "@nestjs/common": "10.3.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.0",
    "@nestjs/platform-express": "10.3.0",
    "@nestjs/terminus": "10.2.0",
    "@prisma/client": "5.7.1",
    "nestjs-cls": "3.6.0",
    "reflect-metadata": "0.1.14",
    "rxjs": "7.8.1",
    "ts-pattern": "^5.0.6",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "10.2.1",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.3.0",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.101",
    "@swc/jest": "0.2.29",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.5",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "cspell": "8.2.3",
    "dependency-cruiser": "15.5.0",
    "dotenv": "16.3.1",
    "dotenv-cli": "7.3.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-prettier": "5.1.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "3.1.0",
    "git-cz": "4.9.0",
    "jest": "29.5.0",
    "npm-package-json-lint": "7.1.0",
    "npm-package-json-lint-config-default": "6.0.0",
    "prettier": "3.1.1",
    "prettier-plugin-sort-json": "3.1.0",
    "prisma": "5.7.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": "20.x",
    "pnpm": "8.x"
  },
  "name": "nestjs-template",
  "private": true,
  "scripts": {
    "build": "nest build -b swc",
    "clean": "rm -rf dist",
    "commit": "git-cz",
    "commit:all": "git add . && pnpm commit",
    "dev": "nest start -b swc --watch",
    "format": "prettier --write .*/*.{js,json} *.{js,ts,json,yml} ./**/*.{js,ts,json,yml} --plugin=prettier-plugin-sort-json",
    "format:check": "prettier --check .*/*.{js,json} *.{js,ts,json,yml} ./**/*.{js,ts,json,yml} --plugin=prettier-plugin-sort-json",
    "format:prisma": "prisma format",
    "gen:prisma": "prisma generate",
    "lint": "eslint \"{prisma,src,test}/**/*.ts\" \"./**/*.{js,json}\"",
    "lint:cspell": "cspell -c cspell.json \"**\" \".*\" \".*/**\"",
    "lint:dep": "depcruise --config .dependency-cruiser.js src",
    "lint:package": "npmPkgJsonLint --configFile ./npmpackagejsonlint.config.js .",
    "lint:type": "tsc --noEmit",
    "prepare:test:e2e": "dotenv -e .env.test -- prisma db push",
    "start": "nest start",
    "test": "jest -c ./jest.config.ts .",
    "test:e2e": "jest -c ./jest.config.ts test",
    "test:ut": "jest -c ./jest.config.ts src"
  },
  "version": "0.0.1"
}
