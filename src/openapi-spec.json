            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ErrorResponse" }
              }
            },
            "description": ""
          }
        },
        "security": [{ "DOCUMENT_JWT_AUTH_NAME": [] }],
        "summary": "get requested user's posts",
        "tags": ["posts"]
      },
