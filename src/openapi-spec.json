            "schema": { "default": 20, "type": "number" }
          },
          {
            "example": 0,
            "in": "query",
            "name": "offset",
            "required": false,
            "schema": { "default": 0, "type": "number" }
          },
          {
            "example": "asc",
            "in": "query",
            "name": "order",
            "required": false,
            "schema": {
              "default": "desc",
              "enum": ["asc", "desc"],
              "type": "string"
            }
          },
          {
            "example": "createdAt",
            "in": "query",
            "name": "orderBy",
            "required": false,
            "schema": {
              "default": "createdAt",
              "enum": ["createdAt", "updatedAt"],
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ListPostResponse" }
              }
            },
            "description": ""
