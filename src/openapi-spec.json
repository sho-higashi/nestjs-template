      "CreatePostDto": {
        "properties": {
          "content": { "type": "string" },
          "title": { "description": "The title of the post.", "type": "string" }
        },
        "required": ["content", "title"],
        "type": "object"
      },
      "CreateTagDto": { "properties": {}, "type": "object" },
      "ErrorResponse": {
        "properties": {
          "path": { "example": "/request/path", "type": "string" },
          "statusCode": { "example": 400, "type": "number" },
          "timestamp": {
            "example": "2023-12-29T16:52:26.720Z",
            "type": "string"
          }
        },
        "required": ["path", "statusCode", "timestamp"],
        "type": "object"
      },
      "ListPostResponse": {
        "properties": {
          "pagination": { "$ref": "#/components/schemas/PaginationDto" },
          "posts": {
            "items": { "$ref": "#/components/schemas/PostResponse" },
            "type": "array"
          },
          "totalCount": { "type": "number" }
        },
        "required": ["pagination", "posts", "totalCount"],
        "type": "object"
      },
      "PaginationDto": {
        "properties": {
          "page": { "type": "number" },
          "perPage": { "type": "number" }
        },
        "required": ["page", "perPage"],
        "type": "object"
      },
      "PaymentWebhookResponse": {
        "properties": { "success": { "type": "boolean" } },
        "required": ["success"],
        "type": "object"
      },
      "PostResponse": {
        "properties": {
          "content": { "type": "string" },
          "createdAt": { "format": "date-time", "type": "string" },
          "id": { "type": "string" },
          "removed": { "type": "boolean" },
          "title": { "type": "string" },
          "updatedAt": { "format": "date-time", "type": "string" }
        },
        "required": [
          "content",
          "createdAt",
          "id",
          "removed",
          "title",
          "updatedAt"
        ],
        "type": "object"
      },
      "RemovePostsDto": { "properties": {}, "type": "object" },
      "UpdateAccountUserDto": { "properties": {}, "type": "object" },
      "UpdateCoffeeDto": { "properties": {}, "type": "object" },
      "UpdateMeDto": {
        "properties": { "name": { "example": "bob", "type": "string" } },
        "required": ["name"],
        "type": "object"
      },
      "UpdatePostDto": {
        "properties": {
          "content": {
            "description": "new content of the post",
            "type": "string"
          },
          "title": { "description": "new title of the post", "type": "string" }
        },
        "type": "object"
      },
      "UpdateTagDto": { "properties": {}, "type": "object" },
      "UserResponse": {
        "properties": {
          "id": {
            "example": "886e8222-9aff-4fd3-87dd-02344242a17c",
            "type": "string"
          },
          "name": { "example": "alice", "type": "string" }
        },
        "required": ["id", "name"],
        "type": "object"
      }
    },
